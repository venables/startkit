{
  "name": "startkit",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "run-s type-check lint format:check test",
    "clean": "git clean -xdf .next",
    "db": "dotenv -c -- drizzle-kit",
    "db:migrate": "dotenv -c -- tsx ./bin/migrate.ts",
    "dev": "next dev",
    "dev:email": "email dev",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "dotenv -c -- next lint",
    "setup": "./bin/setup",
    "start": "next start",
    "test": "dotenv -c -- jest --passWithNoTests",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@auth/core": "^0.12.0",
    "@auth/drizzle-adapter": "^0.3.2",
    "@hookform/resolvers": "^3.2.0",
    "@next/mdx": "^13.4.18",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@react-email/components": "0.0.7",
    "@t3-oss/env-nextjs": "^0.6.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.28.2",
    "lucide-react": "^0.268.0",
    "next": "^13.4.18",
    "next-auth": "^4.23.1",
    "next-themes": "^0.2.1",
    "nextjs-google-analytics": "^2.3.3",
    "nodemailer": "^6.9.4",
    "postgres": "^3.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "1.9.4",
    "react-hook-form": "^7.45.4",
    "resend": "^1.0.0",
    "tailwind-merge": "^1.14.0",
    "ts-deepmerge": "^6.2.0",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/mdx": "^2.0.6",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "autoprefixer": "^10.4.15",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.18",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "next-secure-headers": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.11.3",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",

{
  "name": "startkit",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "concurrently bun:type-check bun:lint bun:format:check bun:test",
    "clean": "git clean -xdf .next",
    "db": "drizzle-kit",
    "db:migrate": "npx tsx ./bin/migrate.ts",
    "dev": "next dev",
    "dev:email": "email dev -p 3001",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "next lint",
    "outdated": "npx npm-check-updates --interactive --format group",
    "postinstall": "patch-package",
    "setup": "./bin/setup",
    "start": "next start",
    "test": "bun test",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@auth/core": "^0.18.0",
